<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= csrf_meta_tag %>
    <%= render 'layouts/stylesheets' %>
  </head>
  
  <body>
    
    <div class="container">
      <%= render 'layouts/header' %>
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>
      
      <section class="round">
        <%= yield %>
      </section>
      
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>



